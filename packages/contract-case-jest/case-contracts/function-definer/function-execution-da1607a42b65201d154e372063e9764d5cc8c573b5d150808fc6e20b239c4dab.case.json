            "triggers": "provided"
          },
          "write": {
            "type": "_case:MockFunctionCaller",
            "stateVariables": "state",
            "triggers": "generated"
          }
        },
        "request": {
          "_case:matcher:type": "_case:Lookup",
          "_case:matcher:uniqueName": "a function invoked with no arguments",
          "_case:matcher:child": {
            "_case:matcher:type": "_case:FunctionArgumentsMatcher",
            "arguments": []
          }
        },
        "response": {
          "_case:matcher:type": "_case:Lookup",
          "_case:matcher:uniqueName": "an object shaped like {success: json stringified 'null'}",
          "_case:matcher:child": {
            "success": {
              "_case:matcher:type": "_case:JsonEncoded",
              "_case:matcher:child": null,
              "_case:matcher:resolvesTo": "string"
            }
          }
        },
        "functionName": "NO ARG FUNCTION"
      },
      "result": "VERIFIED"
    },
    {
