{
  "contractType": "case::contract",
  "description": {
    "consumerName": "function execution",
    "providerName": "function definer"
  },
  "metadata": {
    "_case": {
      "version": "case-internal-tests"
    }
  },
  "matcherLookup": {
    "matcher:a function invoked with no arguments": {
      "_case:matcher:type": "_case:FunctionArgumentsMatcher",
      "arguments": []
    },
    "matcher:an object shaped like {success: json stringified 'null'}": {
      "success": {
        "_case:matcher:type": "_case:JsonEncoded",
        "_case:matcher:child": null,
        "_case:matcher:resolvesTo": "string"
      }
    },
    "matcher:a function invoked with 2 arguments ( \"example\", 2 )": {
      "_case:matcher:type": "_case:FunctionArgumentsMatcher",
      "arguments": [
        "example",
        2
      ]
    },
    "matcher:an object shaped like {success: json stringified '\"example2\"'}": {
      "success": {
        "_case:matcher:type": "_case:JsonEncoded",
        "_case:matcher:child": "example2",
        "_case:matcher:resolvesTo": "string"
      }
    }
  },
  "examples": [
