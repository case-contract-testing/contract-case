{
  "targetDefaults": {
    "prebuild": {
      "dependsOn": [
        "^prebuild"
      ]
    },
    "test": {
      "dependsOn": [
        "downloadContracts",
        "build"
      ],
      "cache": true
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    },
    "build:docs": {
      "dependsOn": [
        "^build",
        "^build:docs",
        "build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "build:docs:local": {
      "dependsOn": [
        "^build",
        "^build:docs:local",
        "build"
      ],
      "outputs": [
        "{projectRoot}/docs"
      ]
    },
    "package": {
      "dependsOn": [
        "build",
        "^package"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    },
    "publish:maven": {
      "dependsOn": [
        "package",
        "^publish:maven"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "lint": {
      "cache": true
    },
    "format:check": {
      "cache": true
    }
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "sharedGlobals": [],
    "production": [
      "default"
    ]
  }
}
